<p id="notice"><%= notice %></p>
<div class="row">  
  <div class="listing col-lg-7 col-lg-offset-1 col-sm-7 full-height">
    <table class="table table-hover table-condensed">
      <thead>
        <tr>
          <th>Assmnt?</th>
          <th>Name</th>
          <th>Address</th>
          <th>Phone</th>
          <th>Email</th>
          <th>Website</th>
          <th colspan="3"></th>
        </tr>
      </thead>

      <tbody>
        <% @listings.each do |listing| %>
          <tr>
            <td>
              <%= link_to listing.image.url do %>
                <%= image_tag listing.image.url, class: "thumbnail" %>
              <% end %>
            </td>
            <td><%= listing.name %></td>
            <td><%= listing.address %></td>
            <td><%= listing.phone %></td>
            <td><%= listing.email %></td>
            <td><%= listing.website %></td>
            <td><%= link_to 'Show', listing, class: "btn btn-link" %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
  <div class="listing-map col-sm-5 col-lg-5 full-height hidden-xs">
    <iframe
      width="100%"
      height="450"
      frameborder="0" style="border:0"
      src="https://www.google.com/maps/embed/v1/place?key=AIzaSyCPvyYbPw07qiiHNJs0JCOr4GLZDu-6ToE
        &q=Space+Needle,Seattle+WA" allowfullscreen>
    </iframe>
  </div>
</div>
<br>
